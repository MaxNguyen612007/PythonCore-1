var = 10+20+30
print(var)
var = 10-20-30
print(var)
var = 10*20*30
print(var)
var = 10/20/30
print(var)
var = 2**2
print(var)
var = 9**(1/2)
print(var)
var = 9%5
print(var)
PI = 3.14159265359
var = round(PI)
print(var)
var = 4**(1/2)
print(var)
a,b,c = 1,2,3
print(a)
print(b)
print(c)
var = 21%9
print(var)
print(1=="1")
print(2!=1)
print(3>2)
print(1<2)
print(2<=2)
